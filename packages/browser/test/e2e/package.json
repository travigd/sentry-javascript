{
  "name": "sentry-browser-integration-tests",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "private": true,
  "scripts": {
    "install-browsers": "playwright install --with-deps",
    "pretest": "yarn install && yarn install-browsers",
    "test:ci": "yarn pretest && playwright test ./suites --browser='all' --reporter='line'",
    "test": "playwright test ./suites",
    "posttest": "rimraf -g suites/**/dist"
  },
  "dependencies": {
    "@playwright/test": "^1.17.0",
    "@sentry/browser": "file:../..",
    "babel-loader": "^8.2.2",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^5.5.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.52.0"
  },
  "resolutions": {
    "@sentry/core": "file:../../../core",
    "@sentry/hub": "file:../../../hub",
    "@sentry/integrations": "file:../../../integrations",
    "@sentry/minimal": "file:../../../minimal",
    "@sentry/tracing": "file:../../../tracing",
    "@sentry/types": "file:../../../types",
    "@sentry/utils": "file:../../../utils"
  }
}
